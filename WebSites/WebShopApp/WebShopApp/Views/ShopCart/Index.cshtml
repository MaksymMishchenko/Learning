@model WebShopApp.ViewModels.ShopCartViewModel
@foreach (var el in Model.ShopCart.ListShopItems)
{
    <div class="alert alert-warning mt-3">
    <b>Car:</b> @el.Car.Name<br />
    <b>Price:</b> @el.Car.Price.ToString("c")<br />
    </div>
}
<hr />
<div class="btn btn-danger" asp-controller="Order">Оплатить</div>