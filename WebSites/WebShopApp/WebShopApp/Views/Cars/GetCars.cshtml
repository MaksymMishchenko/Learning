@using WebShopApp.Controllers
@using WebShopApp.Data.Models
@model IEnumerable<Car>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetCars</title>
</head>
<body>
    <h2>Все автомобили</h2>
@{
    foreach ( var car in Model)
    {
        <div>
            <h2>Model: @car.Name</h2>
            <p>Price: @car.Price.ToString("C")</p>
        </div>
    }

    }
</body>
</html>
